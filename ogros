#include <bits/stdc++.h>

using namespace std;

	int n,m;
	int faixa[10010];
	int pont[10010];
	int F[10010];


int bb(int x){

	int ini = 0, fim = n, meio;

	while(ini <= fim){
		meio = (ini + fim)/2;
		if(meio == fim){
			return fim;
		}else if(meio == ini){
			return ini;
		}else{
			if(x < faixa[meio] and x > faixa[meio - 1]){ 
				return meio-1;
			}
     		if(x < faixa[meio] and x < faixa[meio - 1]){ 
    	 		fim=meio-1;
    	 	}
   	  		if(x > faixa[meio] and x > faixa[meio - 1]){ 
     			ini=meio+1;
     		}
     	}

	}
}

int main(){
	cin>>n>>m;
	faixa[0] = 0;

	for(int i = 1 ; i < n ; i++){
		cin>>faixa[i];
	}
	for(int j = 0 ; j < n ; j++){
		cin>>pont[j];
	}
	for(int k = 0 ; k < m ; k++){
		cin>>F[k];
	}
	for(int a = 0 ; a < m ; a++){
		cout<<pont[bb(F[a])]<<" ";	
	}

	return 0;
}
